<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="email_marketing.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser query" current="2"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="1863"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="Campaign_Performance" custom_title="0" dock_id="3" table="4,20:mainCampaign_Performance"/><dock_state state=""/><default_encoding codec=""/><browse_table_settings><table schema="main" name="Campaign_Performance" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="82"/><column index="2" value="67"/><column index="3" value="76"/><column index="4" value="73"/><column index="5" value="116"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT avg(cp.total_unsubscribes) AS Average_Unsubscribe   -- Overall unsubscribe rate
  FROM Campaign_Performance cp; 

SELECT MAX(cp.total_unsubscribes) AS Highest_Unsubscribe, c.campaign_name  -- Campaign with the highest unsubscribe rate
  FROM Campaign_Performance cp
  JOIN Campaigns c
  ON   cp.campaign_id = c.campaign_id;
  
SELECT MIN(cp.total_unsubscribes) AS Lowest_Unsubscribe, c.campaign_name  -- Campaign with the lowest unsubscribe rate
  FROM Campaign_Performance cp
  JOIN Campaigns c
  ON   cp.campaign_id = c.campaign_id;
  
SELECT c.category, SUM(cp.total_unsubscribes)Total_Unsub -- Campaign category with high unsubscribe rate   
  FROM Campaigns c
  JOIN Campaign_Performance cp
  ON c.campaign_id = cp.campaign_id
  GROUP BY c.category
  ORDER BY Total_Unsub;
  
SELECT c.category, AVG(cp.total_unsubscribes)Avg_Unsubscribe_Rate -- Campaign category with high unsubscribe rate   
  FROM Campaigns c
  JOIN Campaign_Performance cp
  ON c.campaign_id = cp.campaign_id
  GROUP BY c.category
  ORDER BY Avg_Unsubscribe_Rate;
  
SELECT c.campaign_name,cp.total_opens, cp.total_unsubscribes -- Relationship between open rate and unsubscribe rate
   FROM Campaign_Performance cp
   JOIN Campaigns c
   ON c.campaign_id = cp.campaign_id
   GROUP BY c.campaign_name
   ORDER BY cp.total_opens;
 
SELECT COUNT(cp.total_unsubscribes)Total_Unsub, u.device_type  -- Device types with highest unsubscribe rate
  FROM Campaign_Performance cp
  JOIN Unsubscribes un 
  ON un.campaign_id = cp.campaign_id
  JOIN Users u
  ON u.user_id = un.user_id
GROUP BY u.device_type; 
 
 SELECT COUNT(cp.total_unsubscribes)Total_Unsub, u.region  -- Regions with highest unsubscribe rate
  FROM Campaign_Performance cp
  JOIN Unsubscribes un 
  ON un.campaign_id = cp.campaign_id
  JOIN Users u
  ON u.user_id = un.user_id
GROUP BY u.region; 
 
SELECT AVG(un.unsubscribe_date - u.signup_date) AS Client_churn_period   -- How long it takes for users to unsubscribe
FROM Users u
JOIN Unsubscribes un
ON u.user_id = un.user_id;
 
SELECT c.campaign_name, c.send_hour, c.category, cp.total_opens, cp.total_clicks, cp.total_unsubscribes  -- How send time impacts email engagement
FROM Campaigns c
JOIN Campaign_Performance cp
ON c.campaign_id = cp.campaign_id
ORDER BY cp.total_unsubscribes DESC; 

SELECT c.campaign_name, un.reason  -- Reasons for unsubscribe
FROM Campaigns c
JOIN Unsubscribes un
ON un.campaign_id = c.campaign_id
GROUP BY c.campaign_name;

SELECT un.reason, count(un.user_id)  -- Reasons for unsubscribe
FROM Unsubscribes un
GROUP BY un.reason;</sql><current_tab id="0"/></tab_sql></sqlb_project>
